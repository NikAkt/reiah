package pages

import "github.com/denartha10/SummerProjectGOTH/views/layouts"

// This map component is wrapped in a MapView layout so it differs from the other dashboards with no padding
templ Map() {
	@layouts.MapView() {
		<div class="h-screen">
			<div id="map" class="h-full w-full"></div>
		</div>
	}
}
